    "files": [
        "dist"
    ],
    "peerDependencies": {
        "svelte": ">= 5"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "4.0.0",
        "@sveltejs/kit": "2.18.0",
        "@sveltejs/package": "2.3.10",
        "@sveltejs/vite-plugin-svelte": "5.0.3",
        "svelte": "5.23.0",
        "typescript": "5.8.2",
        "vite": "6.2.3"
    },
    "type": "module"
}
